<% article = don_get_object(@article, 'html') %>
<div class="nnenhead">
<%= article.article_date %> /
<%= display_article_categories article %>
<%= article.title_to_html if article.title %> /
</div> <!-- end nnenhead -->
<div class="section">
<%= display_article_images article %>
<%= article.body_to_html if article.body %>
<%= render :template => "shared/comment-noadd", :locals => {:article => article}
 %>
<br clear='all'>
</div> <!-- end section -->

